# PyPI configuration template
# Copy this file to ~/.pypirc and fill in your credentials

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
password = your_pypi_api_token_here

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = your_testpypi_api_token_here